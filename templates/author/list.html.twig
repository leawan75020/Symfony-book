{% extends 'base.html.twig' %} {% block title %}Author - Liste des Authors {%
endblock %} {% block body %}

<h1>Liste des authors</h1>

<ul>
  {% for author in authors %}
  <div>
    <a href="{{ path('app_author_detail', { id: author.id }) }}"
      >Consulter l'auteur {{ author.title }}</a
    >
  </div>

  <li>
    (#{{ author.id }})
    {{ author.nationality }}
  </li>

  {% endfor %}
</ul>

{% endblock %}
